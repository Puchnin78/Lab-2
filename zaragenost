#include <stdio.h>
#include <math.h>
#include <stdlib.h>

using namespace std;

int main() 
{
   float a, b, c, F;
   printf("Введите расстояние между телами = ");
   scanf("%f", &a);
   printf("Введите электрический заряд каждого тела\n");
   printf("Первый заряд = ");
   scanf("%f", &b);
   printf("Второй заряд = ");
   scanf("%f", &c);
   F=(9*(b*c))/a;
   printf("Притяжение двух тел = %.2f", F);
}

